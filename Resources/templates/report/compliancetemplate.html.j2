<h2>Summary Information</h2>
<table class="details">
    <tbody>
    <tr>
    {%- if metadata.overall_status == "All tests passed" %}
        {%- set status_class = "pass" %}
    {%- else %}
        {%- set status_class = "fail" %}
    {%- endif %}
        <th>Status:</th>
        <td class="{{ status_class }}">{{ metadata.overall_status }}</td>
    </tr>
    <tr>
        <th>Start Time:</th>
        <td>{{ metadata.execution_start_time }}</td>
    </tr>
    <tr>
        <th>End Time:</th>
        <td>{{ metadata.execution_end_time }}</td>
    </tr>
    <tr>
        <th>Elapsed Time:</th>
        <td>{{ metadata.execution_elapsed_time }}</td>
    </tr>
    <tr>
        <th>Application Name:</th>
        <td>{{ metadata.app_name }}</td>
    </tr>
    <tr>
        <th>Release:</th>
        <td>{{ metadata.release }}</td>
    </tr>
    <tr>
        <th>Tester:</th>
        <td>{{ metadata.tester }}</td>
    </tr>
    <tr>
        <th>Scope:</th>
        <td>{{ metadata.scope }}</td>
    </tr>
    </tbody>
</table>
<h2>Suite Statistics</h2>
<table class="statistics">
    <thead>
    <tr>
        <th class="stats-col-name">Statistics by Suite</th>
        <th class="stats-col-name">Documentation</th>
        <th class="stats-col-stat">Status</th>
        <th class="stats-col-stat">Total</th>
        <th class="stats-col-stat">Pass</th>
        <th class="stats-col-stat">Fail</th>
        <th class="stats-col-stat">Skip</th>
        <th class="stats-col-elapsed">Elapsed</th>
    </tr>
    </thead>
    <tbody>
    {%- for suite in suites %}
    <tr>
        <td class="stats-col-name">
            <div class="stat-name">{{ suite.name.split('.') | last }}</div>
        </td>
        <td class="stats-col-name">{{ suite.documentation }}</td>
        <td class="stats-col-stat {{ suite.status.lower() }}">{{ suite.status }}</td>
        <td class="stats-col-stat">{{ suite.total }}</td>
        <td class="stats-col-stat">{{ suite.passed }}</td>
        <td class="stats-col-stat">{{ suite.failed }}</td>
        <td class="stats-col-stat">{{ suite.skipped }}</td>
        <td class="stats-col-elapsed">{{ suite.elapsed_time }}</td>
    </tr>
    {%- endfor %}
    </tbody>
</table>
<h2>Test Statistics</h2>
<table class="statistics">
    <thead>
    <tr>
        <th class="stats-col-name">Suite</th>
        <th class="stats-col-name">Test</th>
        <th class="stats-col-stat">Status</th>
        <th class="stats-col-stat-req">Requirements</th>
        <th class="stats-col-elapsed">Elapsed</th>
    </tr>
    </thead>
    <tbody>
    {%- for suite in suites %}
        {%- for test in suite.tests %}
            <tr>
                <td class="stats-col-name">{{ suite.name.split('.') | last }}</td>
                <td class="stats-col-name">{{ test.name }}</td>
                <td class="stats-col-stat {{ test.status.lower() }}">{{ test.status }}</td>
                <td class="stats-col-stat-req">
                {% for item in test.requirements %}
                    {{ item }}<br/>
                {% endfor %}</td>
                <td class="stats-col-stat">{{ test.elapsed_time }}</td>
            </tr>
        {%- endfor %}
    {%- endfor %}
    </tbody>
</table>
</body>
</html>